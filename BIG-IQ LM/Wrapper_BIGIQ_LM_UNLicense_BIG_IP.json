{
	"name":"Wrapper_BIGIQ_LM_UnLicense_BIG-IP",
	"description":"Execute a chained workflow that revokes a BIG-IP License provisioned from BIG-IQ license Manager",
	"globalOptions": {
		"insecure":true,
		"reporters":["cli"]
	},
	"globalVars": {
		"bigiq_username":"admin",
		"bigiq_password":"admin",
		"bigiq_mgmt":"192.168.143.31",
	},
	"globalVarsIncludeFile": "/tmp/Wrapper_BIGIQ_LM_License_BIGIP-env.json",
	"saveEnvVars":true,
	"outputFile":"/tmp/Wrapper_BIGIQ_LM_UNLicense_BIGIP-run.json",
	"envOutputFile":"/tmp/Wrapper_BIGIQ_LM_UNLicense_BIGIP-env.json",
	"workflow": [
		{
			"name":"Authenticate to BIG-IQ LM",
			"options": {
				"collection":"./BIGIQ_License_Manager.postman_collection",
				"folder":"1_Auth"
			}
		},
		{
			"name":"Revoke a license from a BIG-IP",
			"skip":false,
			"options": {
				"collection":"/BIGIQ_License_Manager.postman_collection",
				"folder":"6_Revoke_BIGIP_Device"
			}
		}
	]
}
